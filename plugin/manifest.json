{
  "name": "Hello Extensions",
  "description" : "Base Level Extension",
  "version": "1.0",
  "browser_action": {
    "default_popup": "eco.html",
    "default_icon": "hello_extensions.png"
  },
  "permissions": [
"activeTab",
"<all_urls>"
],
  "manifest_version": 2,
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },

      "description": "Opens eco.html"
    }
  }
}
